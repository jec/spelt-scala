# Informational
GET     /_matrix/client/versions       net.jcain.spelt.controllers.ConfigController.versions()
GET     /.well-known/matrix/client     net.jcain.spelt.controllers.ConfigController.wellKnown()

# Authentication and session
GET     /_matrix/client/v3/login       net.jcain.spelt.controllers.AuthController.loginTypes()
POST    /_matrix/client/v3/login       net.jcain.spelt.controllers.AuthController.logIn()
POST    /_matrix/client/v3/logout      net.jcain.spelt.controllers.AuthController.logOut()
POST    /_matrix/client/v3/logout/all  net.jcain.spelt.controllers.AuthController.logOutAll()
